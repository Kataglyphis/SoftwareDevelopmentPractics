<?xml version="1.0" encoding="UTF-8"?>
<nifty-styles>

    <style id="DefaultButton#panel">
        <attributes childLayout="center" childClip="true" backgroundColor="#FFFFFF" padding="0px,2px"/>
        <effect>
            <onActive name="border" border="4px" color="#000000" post="true" inset="0px"/>
            <onHover name="border" color="#FFFFFF" post="true" inset="-4px"/>
            <onFocus name="border" color="#FFFFFF" post="true" inset="-4px"/>
        </effect>
    </style>

    <style id="DefaultButton#text" base="DefaultText">
        <attributes align="center" valign="center" textHAlign="center" textVAlign="center" visibleToMouse="false"/>
        <effect>
            <onEnabled name="textColorAnimated" startColor="#000000" endColor="#000000" post="false" length="150"/>
            <onDisabled name="textColorAnimated" startColor="#000000" endColor="#000000" post="false" length="150"/>
        </effect>
    </style>

