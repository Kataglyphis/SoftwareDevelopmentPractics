<?xml version="1.0" encoding="UTF-8"?>
<nifty-styles>

    <style id="nifty-progressbar#panel">
        <attributes backgroundImage="edu/kit/valaris/menuSkin/Bilder/Buttons/ButtonProgessBar.png"
                    imageMode="sprite-resize:100,23,2,2,96,2,2,2,96,2,19,2,96,2,2"
                    width="100px" height="23px"  childLayout="center"/>

        <effect>
            <onEnabled name="renderQuad" startColor="#2228" endColor="#2220" post="true" length="150"/>
            <onDisabled name="renderQuad" startColor="#2220" endColor="#2228" post="true" length="150"/>
        </effect>
    </style>

    <style id="nifty-progressbar#indicator">
        <attributes x="160" y="360" backgroundImage="edu/kit/valaris/menuSkin/Bilder/Buttons/ButtonProgessBar.png" align="center" width="23px" height="100%"
                    imageMode="sprite-resize:100,23,1,2,96,2,2,2,96,2,19,2,96,2,2" />
        <effect>
            <onEnabled name="renderQuad" startColor="#2228" endColor="#2220" post="true" length="150"/>
            <onDisabled name="renderQuad" startColor="#2220" endColor="#2228" post="true" length="150"/>
        </effect>
    </style>

    <style id="nifty-progressbar#text">
        <attributes x="50%" y="0" font="edu/kit/valaris/menuSkin/Fonts/Franklin-20.fnt" color="#FFFFFF"
                    textHAlign="center" textVAlign="center" visibleToMouse="false"/>
        <effect>
            <onEnabled name="textColorAnimated" startColor="#8886" endColor="#eeef" post="false" length="150"/>
            <onDisabled name="textColorAnimated" startColor="#eeef" endColor="#8886" post="false" length="150"/>
        </effect>
    </style>

</nifty-styles>
